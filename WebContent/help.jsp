
<%@page import="java.text.DateFormatSymbols"%>
<%@page contentType="text/html" pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>Help</title>
<link rel="stylesheet" type="text/css" href="css/help.css" />
</head>
<body>
	<div id="main_container">
		<div id="main_content">
			<div class="left_content"></div>
			<!-- end of left content -->

			<div class="center_content">
				<div class="center_title_bar">
					<h2>Commands use for Handling Email System</h2>
				</div>
				<div class="prod_box_big">
					<div class="center_prod_box_big">
						<div class="contact_form">
							<div class="form_row">
								<label class="contact"><strong>Help</strong></label> <label
									class="contact1"><strong>For any trouble while
										accessing account set tab and speak "Help"</strong></label>
							</div>
							<div class="form_row">
								<label class="contact"><strong>Forgot password</strong></label>
								<label class="contact1"><strong>set tab and
										speak "Forgot password"</strong></label>
							</div>
							<div class="form_row">
								<label class="contact"><strong>Create Account</strong></label> <label
									class="contact1"><strong>set tab and speak
										"Create Account"</strong></label>
							</div>
							<div class="form_row">
								<label class="contact"><strong>Search result</strong></label> <label
									class="contact1"><strong>set tab and speak
										"content to be search"</strong></label>
							</div>
							<div class="form_row">
								<label class="contact"><strong>Search Button</strong></label> <label
									class="contact1"><strong>set tab and speak
										"search"</strong></label>
							</div>
							<div class="form_row">
								<label class="contact"><strong>Logout</strong></label> <label
									class="contact1"><strong>set tab and speak
										"logout"</strong></label>
							</div>
							<div class="form_row">
								<label class="contact"><strong>Compose new mail</strong></label>
								<label class="contact1"><strong>After login for
										compose mail you have to speak "compose"</strong></label>
							</div>
							<div class="form_row">
								<label class="contact"><strong>Refresh</strong></label> <label
									class="contact1"><strong>speak "Refresh"</strong></label>
							</div>
							<div class="form_row">
								<label class="contact"><strong>Inbox</strong></label> <label
									class="contact1"><strong>speak "inbox"</strong></label>
							</div>
							<div class="form_row">
								<label class="contact"><strong>Send Mail</strong></label> <label
									class="contact1"><strong>speak "send mail"</strong></label>
							</div>
							<div class="form_row">
								<label class="contact"><strong>Draft</strong></label> <label
									class="contact1"><strong>speak "draft"</strong></label>
							</div>
							<div class="form_row">
								<label class="contact"><strong>Trash</strong></label> <label
									class="contact1"><strong> speak "trash"</strong></label>
							</div>
							<div class="form_row">
								<label class="contact"><strong>All mail</strong></label> <label
									class="contact1"><strong>speak "all mail"</strong></label>
							</div>
							<div class="form_row">
								<label class="contact"><strong>Open mail</strong></label> <label
									class="contact1"><strong>set tab and speak
										"open"</strong></label>
							</div>
							<div class="form_row">
								<label class="contact"><strong>Delete mail</strong></label> <label
									class="contact1"><strong>set tab and speak
										"delete"</strong></label>
							</div>
							<div class="form_row">
								<label class="contact"><strong>Back from open
										mail</strong></label> <label class="contact1"><strong>speak
										"back"</strong></label>
							</div>
						</div>
					</div>
				</div>
			</div>
			<!-- end of center content -->
			<div class="right_content"></div>
			<!-- end of right content -->
		</div>
		<!-- end of main content -->
	</div>
	<input type="hidden" id="help" class="pagename" />
	<!-- end of main_container -->
</body>
<script src="js/leftcontent.js"></script>
</html>